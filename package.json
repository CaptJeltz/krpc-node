{
  "name": "krpc-node",
  "version": "1.0.0",
  "description": "Node client lib for krpc",
  "main": "index.js",
  "scripts": {
    "mocha": "mocha --full-trace test/**/*.js",
    "lint": "eslint \"src/**/*.js\" \"test/**/*.js\" index.js --fix",
    "test": "node_modules/.bin/istanbul --include-all-sources cover node_modules/mocha/bin/_mocha test/**/*.js",
    "report": "plato -r -d report -e .eslintrc lib",
    "check-dependencies": "npm-check -Eu"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eXigentCoder/krpc-node.git"
  },
  "keywords": [
    "ksp",
    "krpc",
    "node"
  ],
  "author": "Ryan Kotzen",
  "license": "MIT",
  "dependencies": {
    "protobufjs": "5.0.1",
    "ws": "1.1.1"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "dirty-chai": "1.2.2",
    "eslint": "3.4.0",
    "istanbul": "0.4.5",
    "mocha": "3.0.2",
    "npm-check": "5.2.3",
    "plato": "1.7.0"
  }
}
